{
    "root": "doc/spec",

    "topLevelFilePattern": "(README|CONTENTS)\\.md",
    "linkPattern": "\\[([^\\]]+)\\]\\s*\\(([^\\)]+\\.md)\\)",

    "targetNamePattern": "^#\\s+(.+)",

    "startTestPattern": "^##+\\s+Examples",
    "endTestPattern": "",
    "testNamePattern": "^##+\\s+Example\\s+[0-9]+[\\.\\)]?\\s+(.+)",

    "startCodePattern": "^##+\\s+Code(?:\\s*\\((.+)\\))?",
    "startResultPattern": "^##+\\s+Result.*",

    "codeBlockStart": "^```",
    "codeBlockEnd": "^```",

    "testfile": "test.kx",
    "resultfile": "result.txt",
    "compiler": "",
    "interpreter": "kinx test.kx",
    "interpreterPath": "."
}
