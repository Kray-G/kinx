namespace Typesetting {

    namespace Command {

        class Verb(info_, context_) {
            public exec(params, value) {
                var text = new Typesetting.Word(info_).setWord(context_.fontSet.monotype.fontObj, context_.font.fontSize, value);
                return Typesetting.Linebreak.Box(text.width, text);
            }
        }

    } # namespace Command

} # namespace Typesetting
